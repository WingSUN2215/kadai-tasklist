<h1>Log in</h1>

<%= form_with(url: login_path, scope: :session, local: true) do |f| %>



<div>
    <%= f.label :email, 'Email' %>
    <%= f.text_field :email %>
</div>

<div>
    <%=f.label :password, 'Password' %>
    <%= f.password_field :password %>
</div>
<%= f.submit 'Log in' %>

<div><%= link_to 'New User?', users_path %></div>
<% end %>
